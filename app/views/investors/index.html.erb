<p id="notice"><%= notice %></p>

<h1>Listing Investors</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Company name</th>
      <th>Email address</th>
      <th>Password digest</th>
      <th>Website</th>
      <th>Title</th>
      <th>Profile picture</th>
      <th>Investor</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @investors.each do |investor| %>
      <tr>
        <td><%= investor.name %></td>
        <td><%= investor.company_name %></td>
        <td><%= investor.email_address %></td>
        <td><%= investor.password_digest %></td>
        <td><%= investor.website %></td>
        <td><%= investor.title %></td>
        <td><%= investor.profile_picture %></td>
        <td><%= investor.investor_id %></td>
        <td><%= link_to 'Show', investor %></td>
        <td><%= link_to 'Edit', edit_investor_path(investor) %></td>
        <td><%= link_to 'Destroy', investor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Investor', new_investor_path %>
