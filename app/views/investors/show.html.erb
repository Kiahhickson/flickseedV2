      <div role="tabpanel" class="tab-pane active" id="home">
        <div class="row">

          <div class="col-sm-6 mobile-pull">
              <article role="login">
                <h3 class="text-center"><i class="fa fa-lock"></i>My Profile</h3>
            
                <center>
                  <a href="#aboutModal" data-toggle="modal" data-target="#myModal"><img src="<%= @investor.profile_picture %>" name="aboutme" width="140" height="140" class="img-circle"></a>
                  <h1><%= @investor.name %></h1>
                  <h1><%= @investor.company_name %></h1>
                  <h1><%= @investor.title %></h1>
                  <h1><%= @investor.email_address %></h1>
                  <h1><%= @investor.website %></h1>
                  <%= link_to 'Edit', edit_investor_path(@investor) %>
